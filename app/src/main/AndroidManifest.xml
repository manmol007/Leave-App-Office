<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anmol.leaveapp">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/calender"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/calender"
        android:supportsRtl="true"
        android:theme="@style/MyAppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register"
            android:screenOrientation="portrait"/>
        <activity android:name=".FinalThread"
            android:screenOrientation="portrait"/>
        <activity android:name=".DetailedActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".AppReceiveFinal"
            android:theme="@style/MyAppTheme"
            android:screenOrientation="portrait"/>
        <activity android:name=".Edit"
            android:screenOrientation="portrait"/>
    </application>

</manifest>